package com.fqyuan.java8;

public class Person {
	private String name;
	private String job;
	private String gender;
	private int age;
	private int salary;

	public Person(String name, String job, String gender, int age, int salary) {
		this.name = name;
		this.job = job;
		this.gender = gender;
		this.age = age;
		this.salary = salary;
	}
}
