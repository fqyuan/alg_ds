package com.fqyuan.hashcode;

public class Person {
	private String name;
	private int age;

	public Person(String name, int age) {
		this.name = name;
		this.age = age;
	}

	@Override
	public int hashCode() {
		int result = 1;
		final int prime = 31;
		result = prime * result + age;
		result = prime * result + (name == null ? name.hashCode() : 0);
		return result;
	}

	@Override
	public boolean equals(Object obj){
		if(obj == this)
			return true;
		if(getClass()!=obj.getClass())
			return false;
		Person p =Person(Person)obj;
		return this
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

}
